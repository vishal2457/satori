{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:up": "echo creating && npm run db:create && echo created && npm run db:migrate && echo migrated",
    "db:reset": "npm run db:drop && npm run db:up",
    "db:create": "sequelize db:create",
    "db:drop": "[[ $NODE_ENV == production ]] && exit 1 || sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "db:rollback:all": "sequelize db:migrate:undo:all",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "clean": "rm -rf .turbo && rm -rf node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.1",
    "botbuilder": "^4.18.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^2.3.3",
    "node-statsd": "^0.1.1",
    "ora": "^5.4.1",
    "proses-response": "^1.1.4",
    "rotating-file-stream": "^3.0.4",
    "sequelize": "^6.16.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@types/node-statsd": "^0.1.3",
    "@types/response-time": "^2.3.5",
    "@types/validator": "^13.7.1",
    "@types/vhost": "^3.0.4",
    "dotenv": "^16.0.3",
    "sequelize-cli": "^6.4.1",
    "tsc-watch": "^4.6.0",
    "typescript": "4.7.4"
  }
}
